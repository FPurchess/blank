{
  "name": "blank",
  "private": true,
  "version": "0.2.0",
  "description": "Blank is a minimalistic, opinionated markdown editor made for writing - purely keyboard-based with PDF-Export, available for Linux, macOS and Windows",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "lint": "eslint --ext ts \"./src/**/*.ts\"",
    "lint:fix": "eslint --fix --ext ts \"./src/**/*.ts\"",
    "format": "prettier --write --end-of-line=auto \"./**/*.{cjs,js,jsx,ts,tsx,html,css,json}\" --ignore-path .gitignore --ignore-path .prettierignore",
    "format:check": "prettier --check --end-of-line=auto \"./**/*.{cjs,js,jsx,ts,tsx,html,css,json}\" --ignore-path .gitignore --ignore-path .prettierignore",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "maintainers": [
    {
      "name": "Florian Purchess",
      "email": "florian.purchess@googlemail.com",
      "web": "https://florian-purchess.de/"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/FPurchess/blank.git"
  },
  "resolutions": {
    "w3c-keyname": "^2.2.5"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.2",
    "localforage": "^1.10.0",
    "markdown-it": "^13.0.1",
    "observable.ts": "^1.0.1",
    "pdfmake": "0.3.0-beta.2",
    "prosemirror-commands": "^1.3.0",
    "prosemirror-history": "^1.3.0",
    "prosemirror-keymap": "^1.2.0",
    "prosemirror-markdown": "^1.9.3",
    "prosemirror-schema-basic": "^1.2.0",
    "prosemirror-schema-list": "^1.2.0",
    "prosemirror-state": "^1.4.1",
    "prosemirror-view": "^1.26.3"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.0.3",
    "@types/markdown-it": "^12.2.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "c8": "^7.11.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-security": "^1.5.0",
    "husky": "^8.0.0",
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vitest": "^0.18.0"
  }
}
